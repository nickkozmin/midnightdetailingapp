<%= form_tag(step_2_path(id: @lead.id), method: "get",  class: "form jotform-form form-outline form-outline-black", style:"padding: 20px 20px 15px;" ) do %>
<input type="hidden" name="id" value="<%= @lead.id%>">
  <div class="form-all">
    <ul class="form-section page-section">
     <li class="form-line" data-type="control_dropdown" id="id_14">
      
        <div id="cid_14" class="form-input jf-required">
          <select class="form-control"  id="input_14" name="frequency">
            <option value=""> How often would you like your car cleaned? </option>
            <option value="Once a Month"> Once a Month </option>
            <option value="Once Every 2 Weeks"> Once Every 2 Weeks </option>
            <option value="Once"> Once </option>
          </select>
        </div>
      </li>
      <li class="form-line" data-type="control_fullname" id="id_3">
        
         
       	  <input class="form-textbox" type="text" size="10" data-validation="required" name="first_name" placeholder="First Name" id="first_3" />
       	</li>
            
          
         <li class="form-line" data-type="control_fullname" id="id_3">
           <input class="form-textbox" type="text" size="15" name="last_name" data-validation="required" placeholder="Last Name" id="last_3" />

      </li>
      <li class="form-line" data-type="control_email" id="id_4">
       

          <input type="email" placeholder="Email" data-validation="required" class=" form-textbox validate[Email]" id="input_4" name="email" size="30" value="" />
    
      </li>
      
      <li class="form-line" style="padding:20px;" data-type="control_address" id="id_5">
        
       
          <table summary="" undefined class="form-address-table" border="0" >
            <tr >
             <td style="padding-bottom: 4px; " colspan="2" >
                <span class="form-sub-label-container" style="vertical-align: top;">
                  <input class="form-textbox form-address-line" data-validation="required" placeholder="Street Address" type="text" name="address" id="input_5_addr_line1" value="<%= @lead.address if @lead %>" />
                 
                </span>
             </td>
            </tr>
            <tr>
              <td style="padding-bottom: 4px;" colspan="2">
                <span class="form-sub-label-container" style="vertical-align: top">
                  <input class="form-textbox form-address-line" placeholder="Street Address Line 2" type="text" name="address_line_2" id="input_5_addr_line2" size="46" />
                  
                </span>
              </td>
            </tr>
            <tr style="padding-bottom: 4px;">
              <td width="50%" style="padding-bottom: 4px;" >
                <span class="form-sub-label-container" style="vertical-align: top">
                  <input class="form-textbox form-address-city" data-validation="required" placeholder="City" type="text" name="city" id="input_5_city" size="21" />
                 
                </span>
              </td>
              <td style="padding-left: 5px;">
                <span class="form-sub-label-container" style="vertical-align: top">
                  <input class="form-textbox form-address-state" data-validation="required" placeholder="State/Province" type="text" name="province" id="input_5_state" size="22" />
                  
                </span>
              </td>
            </tr>
            <tr>
              <td width="50%">
                <span class="form-sub-label-container" style="vertical-align: top">
                  <input class="form-textbox form-address-postal" placeholder="Postal / Zip" type="text" name="postal_code" id="input_5_postal" size="10" />
                	
                </span>
              </td>
              <td style="padding-left: 5px;">
                
                  <select class="form-control form-address-country" defaultcountry="" name="country" id="input_5_country">
                    
                    
                    <option value="Canada"> Canada </option>
                    
                  </select>
                 
               
              </td>
            </tr>
          </table>
       
      </li>
      <li class="form-line " style=" padding-top:10px;" data-type="control_checkbox" id="id_13">
        
       
            <input type="hidden" name="condo" value="no">
              <span style="left:0px;">
              <input type="checkbox" style="left:2px;" id="input_13_0" name="condo" value="Yes" />
              <label id="label_input_13_0" for="input_13_0"> This is a condo </label>
          </span>
            
       	
      </li>

      <li class="form-line" data-type="control_phone" id="id_6">
       
        <div id="cid_6" class="form-input jf-required">
        
          <span class="form-sub-label-container" style="vertical-align: top">
            <input class="form-textbox" data-validation="required" placeholder="Phone Number" type="tel" name="phone_number" id="input_6_phone" value="<%= @lead.phone_number if @lead %>" size="10">
            
          </span>
        </div>
      </li>
      <li class="form-line" data-type="control_textbox" id="id_11">
      
        <div id="cid_11" class="form-input jf-required">
          <input type="text" class=" form-textbox" data-type="input-textbox" id="input_11" name="license_plate" placeholder="License Plate/Parking Spot (If in condo)" size="20" value="" />
        </div>
      </li>
 
      
          
      
   	
	<li style="padding-top: 20px; left:0; width: 40px;">        
            <button id="input_2" type="submit" style="left: 0px;" >
              Let's Go
            </button>
      
   </li>
      <li style="display:none">
        Should be Empty:
        <input type="text" name="website" value="" />
      </li>
    </ul>
  </div>

  <%end%>
						
